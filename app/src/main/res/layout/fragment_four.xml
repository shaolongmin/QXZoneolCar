                android:layout_height="wrap_content"
                android:src="@drawable/four_top_add"
                />
        </LinearLayout>

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/four_top_snow"
